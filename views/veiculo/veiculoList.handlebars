<table class="table table-success table-striped table-bordered">
    <tr>
        <td> ID </td>
        <td> Tipo </td>
        <td> Modelo</td>
        <td> Fabricante</td>
        <td> Placa</td>
        <td> Cor</td>
        <td> </td>
        <td> </td>
    </tr>
    {{#each veiculos}}
    <tr>
        <td>{{id}}</td>
        <td>{{tipo}}</td>
        <td>{{modelo}}</td>
        <td>{{fabricante}}</td>
        <td>{{placa}}</td>
        <td>{{cor}}</td>
        <td><a class="btn btn-primary" type="button" href="veiculoEdit/{{id}}">Editar</a></td>
        <td><a class="btn btn-danger" type="button" href="veiculoDelete/{{id}}">Excluir</a></td>
    </tr>
    {{/each}}
</table>
