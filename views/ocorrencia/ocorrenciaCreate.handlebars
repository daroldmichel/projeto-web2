<form action="/ocorrenciaCreate" method="post" class="row g-3 justify-content-center">
    <div class="row-md">
        <div class="col-md-5">
            <label class="form-label" for="observacao">Observação</label>
            <input type="text" class="form-control" id="observacao" placeholder="Observação" name="observacao">
        </div>
    </div>
    <div class="row-md">
        <div class="col-md-5">
            <label class="form-label" for="datahora">Data e Hora</label>
            <input type="datetime-local" class="form-control" id="datahora" placeholder="Data e Hora" name="datahora">
        </div>
    </div>
    <div class="row-md">
        <div class="col-md-5">
            <label class="form-label" for="veiculoId">Veiculo</label>
            <input type="number" class="form-control" id="veiculoId" placeholder="Veiculo" name="veiculoId">
        </div>
    </div>
    <div class="row-md">
        <div class="col-md-5">
            <label class="form-label" for="estacionamentoId">Estacionamento</label>
            <input type="number" class="form-control" id="estacionamentoId" placeholder="Estacionamento" name="estacionamentoId">
        </div>
    </div>
    <div class="row-md">
        <div class="col-md-5">
            <button type="submit" class="btn btn-success">Cadastrar</button>
        </div>
    </div>
</form>
